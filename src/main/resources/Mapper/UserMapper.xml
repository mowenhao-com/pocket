<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pocket.Mapper.UserMapper">


<!-- 分页查询所有审核信息  -->
    <select id="loginAndUserName" resultType="UserData" parameterType="UserData">
          SELECT * FROM sys_user WHERE userName=#{userData.userName} AND `password`=#{userData.password}
    </select>

</mapper>